                </Label>
                <Label fx:id="pass" alignment="CENTER" layoutX="125.0" layoutY="207.0" text="संकेतशब्द :" textFill="BLUE">
                    <font>
                        <Font name="System Bold" size="18.0" />
                    </font>
                </Label>
                <Label fx:id="cpass" alignment="CENTER" layoutX="32.0" layoutY="258.0" text="पुष्टी केलेला संकेतशब्द :" textFill="BLUE">
                    <font>
                        <Font name="System Bold" size="18.0" />
                    </font>
                </Label>
                <TextField fx:id="uname" layoutX="227.0" layoutY="47.0" prefHeight="13.0" prefWidth="284.0" promptText="युजरनेम">
                    <font>
                        <Font size="13.0" />
                    </font>
                </TextField>
                <ComboBox fx:id="squestion" layoutX="227.0" layoutY="97.0" prefHeight="27.0" prefWidth="284.0" promptText="निवडा सुरक्षा प्रश्न" />
